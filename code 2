#ininclude <iostream>

using namespace std;
int main(){
    int *p;
    int arr[]={2,3,5,4};
    p=arr;
    std::cout << "You entered:1"<< std::endl;
    
    
    for ( int i=0; i<4; i++)
    {
       
        std::cout << *p << std::endl;
        p++;
    
    

    
}
return 0 ;
}clude <iostream>
